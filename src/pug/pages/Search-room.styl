.search-rooms-page
    background-color: #FFFFFF;
    width 100%
    min-height: calc(100vh - 372px);
.search-rooms-page__options
    margin-top 30px
    margin-left 20px
    width: 266px;
.search-rooms-page__options > div
    margin-top 30px
    // display grid
    // grid-template-columns: repeat(5, 1fr);
    // grid-column-gap: 30px;
.search-rooms-body
    position relative
    height: 1455px;
    left: 0px;
    top: 0px;
    background: #FFFFFF;
.search-rooms-page__title
    position absolute
    left 40%
    top 0
.search-rooms-page__hotels
    position absolute
    width 900px
    height 1300px
    left 30%
    top 3%
    display grid
    grid-template: 1fr 1fr 1fr 1fr / 1fr 1fr 1fr 
    grid-row-gap: 4px;