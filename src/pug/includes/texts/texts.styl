.texts
    width 413px
    height 321px
.texts__styles
    position absolute
    height: 321px;
    left 67px
    display: flex
    flex-direction: column;
    justify-content: space-between;
.texts__styles_h1
    font-family Quicksand
    font-style normal
    font-weight bold
    font-size 24.1px
    letter-spacing: -0.2px
    line-height 30px
    color $dark100
.texts__styles_h2
    width 345px
    font-family Quicksand
    font-style normal
    font-weight bold
    font-size 19px
    letter-spacing: 0.1px
    line-height 25px
    color $dark100
.texts__styles_h3
    width 186px
    height 15px
    font-family Montserrat, sans-serif
    font-style normal
    font-weight bold
    font-size 12px
    line-height 15px
    text-transform uppercase
    color $dark100
.texts__styles_body
    width 301px
    height 48px
    font-family Montserrat, sans-serif
    font-style normal
    font-weight normal
    font-size 14px
    line-height 24px
    color $dark75

.texts__tegs
    position absolute
    height 273
    width 37px
    height 297px
    display: flex
    flex-direction: column;
    justify-content: space-between;
    align-items: flex-end;
.texts__tegs_H1
    color $dark25
    font-size 24px
    line-height 29px
    margin-bottom: 34px
.texts__tegs_H2
    color $dark25
    font-size 19px
    line-height 23px
    margin-bottom: 29px
.texts__tegs_H3
    color $dark25
    font-size 12px
    line-height 15px
    text-transform uppercase
.texts__tegs_Body
    color $dark25
    font-size 14px
    line-height 24px