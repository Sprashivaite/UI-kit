.header
    position relative
    width 1440px
    height 70px
    background #ffffff
    box-shadow 0px 10px 20px rgba(31, 32, 65, 0.05)
    &__logo_color
        wrap 15px, 140px 
    &__nav
        position absolute
        top 0px
        left 402px
        width 589px
        height 21px
        line-height 17px
        color $dark50
        b
            color: rgba(31, 32, 65, 0.75);
        a
            margin 0px 28px 3px 0px
            float left
            text-decoration none
            color $dark50
            text-decoration none
            &:nth-of-type(4)
                margin-left 17px
    &__expand-more
        wrap 11px, 230px     
        width 24px
        height 21px
        display flex
        align-items center
        text-align center
    &__Button-empty
        wrap 18px, 1100px

