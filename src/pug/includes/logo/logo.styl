.logo
    width 48px
    height 48px
    display inline-block
    position absolute
    left 30px
    top 30px
    &__circle
        position absolute
        width 48px
        height 48px
        border-radius 50%
        border 3px solid $dark25
    &__circle-part1
        position absolute
        width 28px
        height 28px
        right 19.5px
        top 15.25px
        border-radius 46%
        clip rect(0,auto,14px,14px)
        border 2.8px solid $dark25
    &__circle-part2
        position absolute
        width 21px
        height 22px
        left 20px
        top 15.5px
        border-radius 50%
        clip rect(0,13px,6px,3.5px)
        border 2.8px solid $dark25



        