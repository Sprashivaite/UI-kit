.search-rooms
    position: relative
    width: 380px
    height: 374px
    background: #FFFFFF
    border: 1px solid rgba(0, 0, 0, 0.12)
    box-sizing: border-box
    box-shadow: 0px 0px 25px rgba(0, 0, 0, 0.2)
    border-radius: 4px
    &__title
        position: absolute
        width: 320px
        height: 60px
        left: 30px
        top: 40px
        font-family: OpenSans, sans-serif
        font-style: normal
        font-weight: bold
        font-size: 24px
        line-height: 30px
        color: #1F2041
    &__guests
        wrap 205px, 30px
        width: 320px;
        height: 64.56px;
    &__button
        wrap 300px,30px

.search-rooms__com-data
    cont 120px, 30px
.search-rooms__exit-data
    cont 120px, 200px