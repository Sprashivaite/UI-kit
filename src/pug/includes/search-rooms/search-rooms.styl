.search-rooms
    position: relative
    width: 23.75rem
    height: 23.375rem
    background: #FFFFFF
    border: 1px solid rgba(0, 0, 0, 0.12)
    box-sizing: border-box
    box-shadow: 0px 0px 25px rgba(0, 0, 0, 0.2)
    border-radius: 4px
    &__title
        position: absolute
        width: 20rem
        height: 3.75rem
        left: 1.875rem
        top: 2.5rem
        font-family: Montserrat, sans-serif
        font-style: normal
        font-weight: bold
        font-size: 1.5rem
        line-height: 1.875rem
        color: #1F2041
    &__guests
        wrap 12.375rem, 1.875rem
        width: 20rem;
        height: 4.035rem;
    &__button
        wrap 18.75rem,1.875rem

.search-rooms__com-data
    wrap 7.0625rem, 1.875rem