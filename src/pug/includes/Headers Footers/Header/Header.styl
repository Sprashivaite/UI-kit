.Header
    position relative
    width 1440px
    height 70px
    background #ffffff
    box-shadow 0px 10px 20px rgba(31, 32, 65, 0.05)
.Header__logo_color
    wrap 15px, 140px 
.Header__nav
    position absolute
    left 402px
    width 589px
    height 21px
    line-height 17px
    color $dark50
    margin 0px 25px 3px 0px
    a
        position relative
        margin 0px 28px 3px 0px
        float left
        text-decoration none
        color $dark50
        text-decoration none
        &:nth-of-type(4)
            margin-left 17px
.Header__expand-more
    cont 11px, 230px     
    width 24px
    height 21px
    display flex
    align-items center
    text-align center
.Header__Button-empty
    cont 18px, 1100px
