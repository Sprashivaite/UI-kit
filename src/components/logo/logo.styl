.logo
  width 48px
  height 48px
  position relative
  opacity 1
  transition 0.3s
  &:hover
    opacity 0.5
  &__circle
    display block
    width 48px
    height 48px
    border-radius 50%
    border 3px solid $dark25 
    box-sizing border-box
  &__part-left
    position absolute
    right 23.5px 
    top 18px     
    width 28px
    height 28px
    border-radius 46%
    clip rect(0,auto,14px,14px)
    border 3px solid $dark25
  &__part-right
    position absolute 
    left 23px 
    top 19px     
    width 21px 
    height 22px 
    border-radius 50% 
    clip rect(0,13px,6px,3.5px) 
    border 3px solid $dark25